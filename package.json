    "test": "node npm-scripts.js test",
    "coverage": "node npm-scripts.js coverage",
    "release": "node npm-scripts.js release"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testRegex": "test/test.*\\.js"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
